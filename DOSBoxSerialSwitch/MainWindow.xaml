<Window x:Class="DOSBoxSerialSwitch.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:DOSBoxSerialSwitch"
		mc:Ignorable="d"
		Title="DOSBox Serial Switcher" Height="142.373" Width="256.567" ResizeMode="NoResize" Topmost="True">
	<Grid>
		<Button x:Name="ApplyButton" Content="Apply" HorizontalAlignment="Left" Margin="79,79,0,0" VerticalAlignment="Top" Width="75" HorizontalContentAlignment="Center" IsEnabled="False" IsDefault="True" Click="ApplyButton_Click"/>
		<Button x:Name="CloseButton" Content="Close" HorizontalAlignment="Left" Margin="159,79,0,0" VerticalAlignment="Top" Width="75" IsCancel="True" Click="CloseButton_Click"/>
		<Label x:Name="DosBoxLabel" Content="DOSBox" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
		<Label x:Name="label1" Content="Serial Port" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
		<ComboBox x:Name="DosBoxPorts" HorizontalAlignment="Left" Margin="79,13,0,0" VerticalAlignment="Top" Width="155" IsEnabled="False" IsReadOnly="True"/>
		<ComboBox x:Name="SerialPorts" HorizontalAlignment="Left" Margin="79,44,0,0" VerticalAlignment="Top" Width="155" IsReadOnly="True" IsEnabled="False"/>

	</Grid>
</Window>
